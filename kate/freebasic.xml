    <contexts>
      <context attribute="Normal Text" lineEndContext="#stay" name="Normal">


        <RegExpr attribute="SubsNJump" context="#stay" String="^\b(\s*)?(function)([.\s]|$)" insensitive="true" beginRegion="fProcedureRegion"/>
        <RegExpr attribute="SubsNJump" context="#stay" String="\b^(end function)([\s]|$)" insensitive="true" endRegion="fProcedureRegion"/>

        <RegExpr attribute="SubsNJump" context="#stay" String="^\b(\s*)?(sub)([.\s]|$)" insensitive="true" beginRegion="sProcedureRegion"/>
        <RegExpr attribute="SubsNJump" context="#stay" String="\b^(end sub)([\s]|$)" insensitive="true" endRegion="sProcedureRegion"/>

        <RegExpr attribute="Slopes" context="#stay" String="\b^(\s*)?(do)([\s]|$)" insensitive="true" beginRegion="DoRegion"/>
        <RegExpr attribute="Slopes" context="#stay" String="\b^(loop)([\s]|$)" insensitive="true" endRegion="DoRegion"/>
        
        <RegExpr attribute="Slopes" context="#stay" String="\b^(\s*)?(open)([\s]|$)" insensitive="true" beginRegion="OpenRegion"/>
        <RegExpr attribute="Slopes" context="#stay" String="\b^(close)([\s]|$)" insensitive="true" endRegion="OpenRegion"/>
        

        <RegExpr attribute="Slopes" context="#stay" String="^(\s*)?(for)([\s]|$)" insensitive="true" beginRegion="ForRegion"/>
        <RegExpr attribute="Slopes" context="#stay" String="\b(next)([\s]|$)" insensitive="true" endRegion="ForRegion"/>        

        
        <RegExpr attribute="Branches" context="#stay" String="^(\s*)?if([\s])" insensitive="true" beginRegion="IfRegion" /> 
        <RegExpr attribute="Branches" context="#stay" String="\b(then\s+)(?!(rem|$|'))" insensitive="true" endRegion="IfRegion" />
        <RegExpr attribute="Branches" context="#stay" String="\b((end if)|(endif))([\s]|$)" insensitive="true" endRegion="IfRegion"/>   
        
        
        <RegExpr attribute="Branches" context="#stay" String="\b(\s*)?(select)([\s]|$)" insensitive="true" beginRegion="SelectRegion"/>
        <RegExpr attribute="Branches" context="#stay" String="\b(end select)([\s]|$)" insensitive="true" endRegion="SelectRegion"/>
        
        
        
        <RegExpr attribute="Data Types" context="#stay" String="\b(type)([\s]|$)" insensitive="true" beginRegion="StructureRegion"/>
        <RegExpr attribute="Data Types" context="#stay" String="\b(end type)([\s]|$)" insensitive="true" endRegion="StructureRegion"/>
        
        <RegExpr attribute="Data Types" context="#stay" String="\b(with)([\s]|$)" insensitive="true" beginRegion="WithRegion"/>
        <RegExpr attribute="Data Types" context="#stay" String="\b(end with)([\s]|$)" insensitive="true" endRegion="WithRegion"/>
        
        
        <RegExpr attribute="Assembly Operators" context="#stay" String="\b(asm)([\s]|$)" insensitive="true" beginRegion="AsmRegion"/>
        <RegExpr attribute="Assembly Operators" context="#stay" String="\b(asm)([\s])" insensitive="true"/>
        <RegExpr attribute="Assembly Operators" context="#stay" String="\b(end asm)([\s]|$)" insensitive="true" endRegion="AsmRegion"/>
    
        <RegExpr attribute="Slopes" context="#stay" String="^(\s*)?(while)([\s]|$)" insensitive="true" beginRegion="WhileRegion"/>
        <RegExpr attribute="Slopes" context="#stay" String="^(\s*)?(wend)([\s]|$)" insensitive="true" endRegion="WhileRegion"/>
        
        <RegExpr attribute="Slopes" context="#stay" String="^(\s*)?(repeat)([\s]|$)" insensitive="true" beginRegion="RepeatRegion"/>
        <RegExpr attribute="Slopes" context="#stay" String="^(\s*)?(until)([\s]|$)" insensitive="true" endRegion="RepeatRegion"/>
        
        

       
        <keyword attribute="preproc" context="#stay" lineEndContext="#pop" String="Preprocessor"/>
